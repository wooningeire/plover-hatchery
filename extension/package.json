{
  "name": "hatchery-language-support",
  "displayName": "Hatchery language support",
  "description": "",
  "version": "0.0.0",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "hatchery",
        "aliases": [
          "Hatchery dictionary",
          "hatchery"
        ],
        "extensions": [
          ".hatchery"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "hatchery",
        "scopeName": "meta.key.injection.hatchery",
        "path": "./out/hatchery.key.injection.tmLanguage.json",
        "injectTo": ["source.hatchery"]
      },
      {
        "language": "hatchery",
        "scopeName": "meta.sopheme.injection.hatchery",
        "path": "./out/hatchery.sopheme.injection.tmLanguage.json",
        "injectTo": ["source.hatchery"]
      },
      {
        "language": "hatchery",
        "scopeName": "source.hatchery",
        "path": "./out/hatchery.file.tmLanguage.json"
      }
    ]
  },
  
  "devDependencies": {
    "@vscode/vsce": "^3.3.2"
  }
}
